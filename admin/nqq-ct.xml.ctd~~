    <rich_text>


flameshot gui
	flameshot gui -p ~/myStuff/captures

	#!/bin/bash
	TMP=$(mktemp /tmp/XXXXXXX.png)
	flameshot gui --clipboard --raw &gt; $TMP
	filename=$(dmenu -fn "Terminus (TTF):pixelsize=28" -p "Screenshot filename" &lt; /dev/null)
	if test "$filename"
	then
	output="/tmp/$filename.png"
	mv -v "$TMP" $output
	upload "$output"
	fi
	


